{
  "templater": {
    "command_timeout": 5,
    "trigger_on_file_creation": false,
    "auto_jump_to_cursor": true,
    "enable_ribbon_icon": true,
    "enable_system_commands": false,
    "shell_path": "",
    "user_scripts_folder": "",
    "user_functions_folder": "",
    "enable_folder_templates": true,
    "folder_templates": [
      {
        "folder": "",
